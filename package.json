{
  "name": "react-json-tree",
  "version": "1.0.0-beta.0",
  "description": "React JSON Viewer Component",
  "main": "lib/index.js",
  "scripts": {
    "precommit": "npm -s run lint && lint-staged",
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib",
    "lint": "eslint src test",
    "flow": "flow",
    "test": "NODE_ENV=test ava --verbose",
    "test-nyc": "nyc ava",
    "test-cov": "nyc ava && nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "prepublish": "npm -s run clean && npm -s run build",
    "start": "cd packages/react-json-tree-demo-app && npm start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexkuz/react-json-tree.git"
  },
  "keywords": [
    "react",
    "json viewer"
  ],
  "author": "Shu Uesugi <shu@chibicode.com> (http://github.com/chibicode)",
  "contributors": [
    "Alexander Kuznetsov <alexkuz@gmail.com> (http://kuzya.org/)",
    "Dave Vedder <veddermatic@gmail.com> (http://www.eskimospy.com/)",
    "Daniele Zannotti <dzannotti@me.com> (http://www.github.com/dzannotti)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chibicode/react-json-tree/issues"
  },
  "homepage": "https://github.com/alexkuz/react-json-tree",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "codecov": "^1.0.1",
    "eslint": "^3.17.1",
    "eslint-config-prettier": "^1.5.0",
    "eslint-config-standard": "^7.0.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.1.1",
    "expect": "^1.18.0",
    "husky": "^0.13.2",
    "lerna": "2.0.0-beta.37",
    "lint-staged": "^3.3.1",
    "nyc": "^10.1.2",
    "prettier": "^0.22.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "rimraf": "^2.5.2"
  },
  "peerDependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "react-base16-styling": "^0.5.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
